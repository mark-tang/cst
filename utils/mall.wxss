}
.chose-adress__text_main text:first-child{
  max-width: 70%;
  min-width: 40%;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1;
  overflow:hidden;
}
.chose-adress__text_sub{
  font-size: 30rpx;
  color: #999;
  height: 50rpx;
  line-height: 50rpx;
  display:-webkit-box;
  -webkit-box-orient:vertical;
