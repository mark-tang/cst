/*mall-media*/
.mall-media{
  padding: 20rpx;
  box-sizing: border-box;
  position: relative;
  display: flex;
  align-items: center;
  background-color: #FFFFFF;
  overflow: hidden;
}
.mall-media::after{
  content:'';
  position: absolute;
  border: 1rpx solid #e6e6e6;
  width: 100%;
  bottom: -1rpx;
  left: 230rpx; 
  transform: scaleY(0.3)
}

.mall-media__lt{
  width: 230rpx;
  height: 200rpx;
  text-align: center; 
  position: relative;
  flex-shrink: 0;
  border: 1rpx solid #f8f8f8;
}
.mall-media__rt{
  width: 100%;
  padding-left: 30rpx;
}
.mall-media__lt image{
  width: 230rpx;
  height: 200rpx;
}
.mall-media__lt__type{
  position: absolute;
  width: 66rpx;
  height: 36rpx;
  line-height: 36rpx;
  color: #FFF;
  font-size:22rpx; 
  text-align: center;
  top: 10rpx;
  left: 0;
  z-index: 100;
}
.mall-media__lt__type.type_3{
  background-color: #ee3030;
}
.mall-media__lt__type.type_2{
  background-color: #ff8a14;
}
.mall-media__lt__type.type_1{
  background-color: #15c679;
}
.mall-media__hd{
  font-size: 34rpx;
}

.mall-media__ft{
  display: flex;
  justify-content: space-between;
}
.mall-media__ft__lt{
  width: 55%;
  display: flex;
  flex-direction:column;
}
.mall-media__ft__btn{
  width: 40%;
}

.mall-media-title{
  font-size: 28rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.mall-media__price{
  display: flex;
  line-height: 60rpx;
}
.mall-media__price-red{
  color: #e54b44;
  font-size: 26rpx;
}
.mall-media__price-orgin{
  font-size: 22rpx;
  color: #999;
  margin-left: 20rpx;
}
.text-del{
  text-decoration: line-through;
}
.mall-media__price-red text{
  font-size: 38rpx;
  font-weight: bold;
}

.mall-media-cell{
  width: 100%;
}
.mall-media-cell_text{
  font-size: 24rpx;
  color: #999;
  width: 100%;
  padding-bottom: 5rpx;
  line-height: 1;
}
/*进度条*/
.cell_process{
  height: 15rpx;
  background-color:#f0f0f0;
  border-radius: 100rpx;
  overflow: hidden;
  margin-top: 10rpx;
  width: 80%;
}
.cell_process-inner{
  background: #ff4e46;
  height: 15rpx;
  border-radius: 100rpx;
  transition:all .6s;
}
/*倒计时*/
.cell_cut-down{
  font-size: 24rpx;
  color: #999;
  margin-top: 5rpx;
}
.cell_cut-down view{
  display: inline;
}
.cell_cut-down text{
  background-color: #858585;
  color: #FFF;
  font-size: 24rpx;
  padding: 0rpx 5rpx;
  min-width: 24rpx;
  display: inline-block;
  text-align: center;
  border-radius: 5rpx;
  height: 1.4;
  line-height: 1.4;
}
/*按钮*/
.mall-media__btn{
  line-height: 2.4;
  background: #ff4e46;
  color: #FFF;
  border-radius: 6rpx;
  font-size: 28rpx;
}
.mall-media__btn-gray{
  background: #c7c7c7;
}
.mall-media__btn::after{
  display: none;
}
/*mall-panel*/
.mall-panel_list-title{
  padding: 20rpx;
