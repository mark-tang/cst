  overflow:hidden;
}
.adress-top-image{
  height: 5rpx;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
}
.adress-image{
  width: 40rpx;
  height: 49rpx;
  position: absolute;
  top: 50%;
  left: 30rpx;
  margin-top: -25rpx;
}
.arrow_right{
  padding-right:100rpx;
  box-sizing: border-box;
  position: relative;
}
.arrow_right::after{
  content: '';
  position: absolute;
  width: 20rpx;
  height: 20rpx;
  border-top: 2rpx solid #bdbdbd;
  border-right: 2rpx solid #bdbdbd;
  right: 30rpx;
  top: 50%;
  margin-top: -10rpx;
  transform: rotate(45deg);
  border-radius: 3rpx;
}
