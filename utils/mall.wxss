mycheckbox{
  width: 40rpx;
  height: 40rpx;
  background: #FFF;
  box-shadow: inset 0 0 0 2rpx #AAA;
  display: block;
  border-radius: 50%;
  transition: .2s ease-in-out;
}
mycheckbox.active{
  box-shadow: inset 0 0 0 24rpx #e54b44;
  position: relative;
  transition: .2s ease-in-out;
}

mycheckbox.active::after{
  content: '';
  background-image: url(data:image/png;base64,);
  width: 32rpx;
  height: 32rpx;
  display: block;
  z-index: 2;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  position: absolute;
  left: 6rpx;
  top: 6rpx;
}

.fixed-btn{
