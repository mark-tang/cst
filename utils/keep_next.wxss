.keep-tool{
  position: relative;
  margin-bottom: 120rpx;
}
.keep-normal{
  margin-bottom: 40rpx;
}
.keep-tool button{
  margin-top: 60rpx;
  box-shadow: 0 0 #FFF;
}

.keep-tool text{
  margin: 40rpx 0;
  display: block;
  text-align: center;
  font-size: 30rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: contain;
  width: 240rpx;
  position: absolute;
  left: 50%;
  margin-left: -110rpx;
  padding-left: 10rpx;
}

.card-title{
  text-align: center;
  font-size: 28rpx;
  font-weight: bold;
  display: block;
  padding: 20rpx 0;
}
.card-titl-image{
  width: 79rpx;
  height: 10rpx;
  transform: translateY(-5rpx);
  padding: 0 15rpx;
}

.keep-list-item{
  display: flex;
  padding: 20rpx 0;
}
.keep-list-item:first-child{
   border-bottom: 1rpx solid #ececec
}
.keep-list-cell{
  width: 50%;
  text-align: center;
}
.keep-list-cell-full{
  width: 100%;
}
.keep-list-title{
  font-size: 24rpx;
  color: #9b9b9b;
  
}
.keep-list-con{
  font-size: 34rpx;
  display: block;
  margin: 15rpx 0;
}
.keep-list-con-red{
  color: #f91540;
}

/*提示*/
.tips{
  color: #9b9b9b;
  font-size: 28rpx;
  line-height: 48rpx;
  margin-top: 50rpx;
  padding: 0 30rpx;
}
.tips text{
  color: #333;
}
.tips view{
  font-size: 26rpx;
  line-height: 38rpx;
}
