@import '../../keep/select_car/select_car.wxss';

.offcanvas {
  position: fixed;
  z-index: 1091;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: #f5f5f5;
  width: 600rpx;
}

.offcanvas_right {
  left: auto;
  right: 0;
  -webkit-transform: translateX(100%);
  -ms-transform: translateX(100%);
  transform: translateX(100%);
}