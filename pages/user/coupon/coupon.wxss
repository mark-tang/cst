/** 重写nav-area样式**/
.nav-area{
  width: 100%;
  height: 39px;
  position: fixed;
  background: #fff;
  z-index: 999;
}
.nav-wrap{
  display: flex;
  justify-content: space-around;
  align-items: center;
  height: 100%;
}
.nav-item{
  width: 45px;
  line-height: 38px;
  height: 100%;
  font-size: 14px;
  box-sizing: border-box;
}
.nav-active{
  color: #228ade;
  border-bottom: 2px solid #228ade;
}

.fixed-btn.fixed-card-btn{
  left: 0;
  right: 0;
  bottom: 0;
  padding-bottom: 30rpx;
  background-color: #f8f8f8;
}

.card-list-btn{
  font-size: 16px;
  display:flex;
  flex-direction:column;
}
.btn-tb{
  height: 21px;
}
.btn-tips{
  font-size: 12px;
}
.no-border{
  border: 0;
}
        