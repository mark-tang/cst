/**index.wxss**/
.container{
  padding: 0;
}
.qrcode-wrap {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  background: rgba(0, 0, 0, .4);
  z-index: 999;
}
.qrcode {
  width: 420rpx;
  height: 500rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: center;
  background: #FFF;
  border-radius: 10rpx;
  position: relative;
}
.qrcode text {
  font-size: 32rpx;
  padding: 15rpx 0;
  color: #999;
}
.qrcode > canvas {
  width: 420rpx;
  height: 420rpx;
}
.coupon-list-wrap{
  width: 100%;
  height: 100%;
  overflow: hidden;
  overflow-y:scroll;
  -webkit-overflow-scrolling: touch;
  padding:  80rpx 0 20rpx 0;
  box-sizing: border-box;
}
.coupon-list-wrap.has-padding{
  padding:  80rpx 0 140rpx 0;
}
.coupon-list{
  width: 96%;
  margin: 20rpx auto;
  display: flex;
  padding: 26rpx 0rpx;
  box-sizing: border-box;
  background-size:100% 100%;
  background-repeat: no-repeat;
  background-image: url(data:image/png;base64,)
}
.coupon-list.noUse, .coupon-list.now{
  background-image:url(data:image/png;base64,);

}

.coupon-list.apply{
  background-image: url(data:image/png;base64,)
}
.coupon-list.overdue{
  background-image:url(data:image/png;base64,)
}
.coupon-list.used{
  background-image:url(data:image/png;base64,)
}
.coupon-list-left{
  width: 75%;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.coupon-left-title{
  font-size: 32rpx;
  font-weight: bold;
  padding: 10rpx 0;
  color: #242424;
}
.coupon-left-cell{
  color: #666;
  font-size: 24rpx;
  padding: 4rpx 0;
}
.ellipsis-span{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient: vertical;
}
.coupon-list-right{
  width: 25%;
  color: #FFF;
  text-align: center;
  padding: 0 0rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.coupon-right-money{
  font-size: 38rpx;
  font-weight: bold;
  margin-top: 30rpx;
}
.coupon-right-money text{
  font-size: 28rpx;
}
.coupon-right-status{
  border: 1px solid #fff;
  border-radius: 100rpx;
  font-size: 24rpx;
  padding: 10rpx 10rpx;
  margin: 0 auto;
  margin-top:36rpx;
  width: 80%;
}


