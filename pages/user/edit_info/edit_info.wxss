.frameImage image{
  z-index: 1010;
  width: 80%;
  height: 53%;
  position: fixed;
  top: 45%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.text-gray {
  display: block;
  font-size: 28rpx;
  color: #9b9b9b;
  text-align: center;
  line-height: 2rem;
}

.select-advisor-title{
  font-size: 30rpx;
  color: #9b9b9b;
  padding: 30rpx;
  display: block;
} 
.select-advisor{
  background: #FFF;
  display: flex;
  flex-wrap:wrap;
  padding: 20rpx 0 0rpx 0;
  margin-bottom: 100rpx;
}
.advisor-info{
  width: 25%;
  text-align: center;
  font-size: 24rpx;
  color: #333;
  padding-bottom:20rpx;
  position: relative;
}
.advisor-info .advisor-pic{
  width: 130rpx;
  height: 130rpx;
  margin: 0 auto;
  border-radius: 50%;
  overflow: hidden;
  border: 3px solid #fff;
  box-sizing: border-box;
}
.advisor-info.active .advisor-pic{
  border: 3px solid #2c92ed;
}
.advisor-info text{
  display: block;
  margin-top: 10rpx;
}
.selected{
  position: absolute;
  height: 40rpx;
  width: 40rpx;
  background: #2c92ed;
  border-radius:50%; 
  bottom: 60rpx;
  right: 30rpx;
  text-align: center;
  display: none;
}
.advisor-info.active .selected{
  display: block;
}
.selected image{
  width: 24rpx;
  height:20rpx;
  margin-top: 10rpx;
}

