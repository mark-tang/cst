  width:50rpx;
}
.form-special-label{position: absolute}
.pro-select-wrap{
  position: fixed;
  bottom: 0;
  background: #d9dcdf;
  padding: 20rpx;
  display: flex;
  flex-wrap: wrap;
  box-shadow: 0 0 15rpx #d9dcdf;
  z-index: 1000001;
  transition: all .3s ease-in-out;
  transform:translateY(100%);
}
.pro-select-wrap.active{
  transition: all .3s ease-in-out;
  transform: translateY(0)
}
.pro-select{
  width: 12.5%;
  display: block;
  margin: 30rpx 0;
  text-align: center;
}
.pro-select text{
  background: #FFF;
  padding: 20rpx 15rpx;
  border-radius: 6rpx;
  box-shadow: 0 3rpx 3rpx #848688 ;
}
.pro-select text:active{
  background: #efefef;
  box-shadow: 0 1rpx 1rpx #848688 ;
  color: #3f69f7;
}