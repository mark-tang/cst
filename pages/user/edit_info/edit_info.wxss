.radio24{margin-right:24rpx}
.fast-provice{
  position: absolute;
  width: 60rpx;
  padding:0 10rpx;
  height: 40rpx;
  line-height: 40rpx;
  left: 482rpx;
  top: 50%;
  margin-top: -20rpx;
  border: 2rpx solid #3f69f7;
  color: #3f69f7;
}

.fast-provice image{
  width: 16rpx;
  height: 9rpx;
  position: absolute;
  right: 15rpx;
  top: 50%;
  margin-top: -5rpx;
}
