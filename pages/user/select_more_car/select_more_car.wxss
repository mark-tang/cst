.container{
  padding: 0;
}
.search-wrap{
  width: 100%;
  background: #FFF;
  padding: 20rpx 0;
  display: flex;
}
.search-wrap .search-input{
  width: 75%;
  background: #efefef;
  text-indent: 20rpx;
  border: 1px solid #f8f8f8;
  padding:10rpx 20rpx;
  height:70rpx;
  line-height: 70rpx;
  border-radius: 10rpx;
  font-size: 32rpx;
  box-sizing: border-box;
  margin: 0rpx auto;
  justify-content: space-between;
}
.search-wrap button{
  width: 15%;
  height: 65rpx;
  line-height: 65rpx;
  font-size: 26rpx;
  margin-top: 5rpx;
  background: #426cf8;
  color: #FFF;
}
.type-list-wrap{
  border-top: 1rpx solid #ececec;
  padding-left: 15rpx;
  margin-bottom: -1rpx;
  background: #FFF;
}
.type-list{
  padding: 10rpx 30rpx;
  border-bottom: 1rpx solid #ececec;
  box-sizing: border-box;
  display: flex;
  position: relative;
}
.type-list image{
  height: 80rpx;
  width: 100rpx;
}
.type-list text{
  height: 80rpx;
  line-height: 80rpx;
  padding-left:40rpx;

}
.type-list image.arrow-right{
  position:absolute;
  height:30rpx;
  width:16rpx;
  right:20rpx;
  top:50%;
  margin-top:-15rpx;
}
