/* pages/keep/member_charge/member_charge.wxss */
.form-list{
  padding:0 24rpx;
  display:flex;
  flex-wrap :wrap;
  justify-content:flex-start;
  width: 92%;
  margin: 0 auto;
  color: #228ade;
}
 .form-item{
  width: 32%;
  height: 120rpx;
  margin-bottom:  3%;
  margin-right: 2%;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  /* border-radius: 7px;
  border: 1px solid #228ade; */
   background: url(data:image/png;base64,) center no-repeat;
  background-size: 100% 100%; 
  overflow: hidden;
  box-sizing: border-box;
}
.form-item:nth-of-type(3n + 3){
  margin-right: 0;
}
.snum{
  font-size: 28rpx;
}
 .item-act{
  background: url(data:image/png;base64,) center no-repeat;
  background-size: 100% 100%;
}  

.charge-history{
  position: fixed;
  bottom: 0;
  width: 100%;
  font-size: 28rpx;
  text-align: center;
  padding: 30rpx 0;
  color: #228ade;
}