.frameImage image{
  z-index: 1010;
  width: 80%;
  height: 53%;
  position: fixed;
  top: 45%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.picker{
  
}
.form-section picker{ 
  width: 50%;
  line-height: 1.4rem
}
.city-picker-first{
  left: 50%;
  margin-left: 40rpx;
}
.city-picker-last{
  margin-right: 60rpx;
}

.fast-provice{
  position: absolute;
  width: 60rpx;
  padding:0 10rpx;
  height: 40rpx;
  line-height: 40rpx;
  left: 200rpx;
  top: 50%;
  margin-top: -20rpx;
  border: 2rpx solid #3f69f7;
  color: #3f69f7;
}
.fast-provice image{
  width: 16rpx;
  height: 9rpx;
  position: absolute;
  right: 15rpx;
  top: 50%;
  margin-top: -5rpx;
}
.fast-provice-input{
  width: 60%;
  margin-left:20%;
}
.pro-select-wrap{
  position: fixed;
  bottom: 0;
  background: #d9dcdf;
  padding: 20rpx;
  display: flex;
  flex-wrap: wrap;
  box-shadow: 0 0 15rpx #d9dcdf;
}
.pro-select{
  width: 12.5%;
  display: block;
  margin: 30rpx 0;
  text-align: center;
}
.pro-select text{
  background: #FFF;
  padding: 20rpx 15rpx;
  border-radius: 6rpx;
  box-shadow: 0 3rpx 3rpx #848688 ;
}
.pro-select text:active{
  background: #efefef;
  box-shadow: 0 1rpx 1rpx #848688 ;
  color: #3f69f7;
}