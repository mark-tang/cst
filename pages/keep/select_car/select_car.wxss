/**keep.wxss**/
.car{
  width: 100%;
  padding: 30rpx;
  background: #FFF;
}

.band-list{
  width: 100%;
  background: #FFF;
}
.band-title{
  padding: 10rpx 30rpx;
  border-top: 1rpx solid #ececec;
  
  box-sizing: border-box;
  display: flex;
  position: relative;
}
.band-title image{
  height: 80rpx;
  width: 80rpx;
}
.band-title text{
  height: 80rpx;
  line-height: 80rpx;
}
.car-band{
  font-size: 34rpx;
  display: block;
  padding-left: 40rpx;
}
.car-band-tips{
  font-size: 28rpx;
  color: #b2b2b2;
  position: absolute;
  right: 30rpx;
}
.type-list-wrap{
  border-top: 1rpx solid #ececec;
  padding-left: 45rpx;
  margin-bottom: -1rpx;
}
.type-list{
  padding: 10rpx 30rpx;
  border-bottom: 1rpx solid #ececec;
  box-sizing: border-box;
  display: flex;
  position: relative;
}
.type-list image{
  height: 80rpx;
  width: 100rpx;
}
.type-list text{
  height: 80rpx;
  line-height: 80rpx;
  padding-left:40rpx;

}
.other-car{
  padding: 10rpx 30rpx;
  border-top: 1rpx solid #ececec;
  border-bottom: 1rpx solid #ececec;
  box-sizing: border-box;
  display: flex;
  position: relative;
  background: #FFF;
}
.other-car image{
  height: 10rpx;
  width: 44rpx;
  position: absolute;
  top: 50%;
  margin-top: -5rpx;
}
.other-car text{
  height: 80rpx;
  line-height: 80rpx;
  padding-left:70rpx;
}