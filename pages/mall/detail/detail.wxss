@import "/utils/mall.wxss";
.container{
  padding: 0;
  padding-bottom: 200rpx;
}
.flex-wrp{display: flex;flex-direction: row;margin:0!important;}
.flex-item{flex:1;height:60rpx;color:#fff;justify-content: center;
  align-items: center;text-align:center;line-height:60rpx;background: #39d495;display: flex;}
  .flex-item2{flex:1}
swiper{
  height: 647rpx;
  background: #f0f0f0;
}
.slide-image{
  width: 100%;
  height: 647rpx;
  background: #FFF;
}

.detail-top{
  background: #FFF;
  
  padding:15rpx 20rpx;
}
.detail-top__hd{
  
} 
.detail-top__bd{
  display: flex;
  justify-content: space-between;
  padding: 20rpx 0;
}

.detail-panel__title{
  font-size: 30rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  margin: 5rpx 0;
  line-height: 46rpx;
}

.detail-top__bd__lt{
  display: flex;
}
  .price-red{
    color: #e54b44;
    font-size: 26rpx;
    line-height: 40rpx;
  }
  .price-orgin{
    font-size: 22rpx;
    text-decoration: line-through;
    color: #999;
    margin-left: 20rpx;
    margin-top: 10rpx;
  }
  .price-red text{
    font-size: 38rpx;
    font-weight: bold;
  }
.detail-top__bd__rt{
  font-size: 26rpx;
  color: #999;
  line-height: 44rpx;
}
.detail-cutdown view{
  display: inline;
}
.detail-cutdown text{
  background-color: #e54b44;
  color: #FFF;
  font-size: 24rpx;
  padding: 0rpx 5rpx;
  min-width: 24rpx;
  display: inline-block;
  text-align: center;
  border-radius: 5rpx;
  line-height: 1.2;
}
.detail-top__ft{
  font-size: 26rpx;
  color: #999;
  display: flex;
  justify-content: space-between;
  
}


/*detail tab*/
.detail-tab{
  background: #FFF;
  margin-top: 20rpx;
}
.detail-tab__hd{
  border-bottom: 1rpx solid #ededed;
  height: 100rpx;
  line-height: 100rpx;
  position: relative;
}
.detail-tab__hd .tab__hd_title{
  width: 25%;
}
.detail-tab__hd .tab__hd_dot{
  width: 25%;
  margin-left: 0%;
}
/*detail bar*/
.detail-bar{
  display: flex;
  position: fixed;
  bottom: 0;
  left: 0;
  background: #FFF;
  height: 98rpx;
  /* border-top: 1rpx solid #ededed; */
  width: 100%;
  box-shadow: 0 0 5rpx #999;
}
.detail-bar__cell__shopcar{
  position: relative;
  width: 150rpx;
  flex-shrink: 0;
}
.detail-bar__cell__shopcar image{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}

.shopcar__num{
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  border-radius: 50%;
  position: absolute;
  background: #ff544c;
  color: #FFF;
  font-size: 22rpx;
  text-align: center;
  right:30rpx;
  top: 20rpx;
  z-index: 100;
}
.detail-bar__handle{
  width: 100%;
  display: flex;
}
.detail-bar__cell{
  color: #FFF;
  text-align: center;
  line-height: 98rpx;
  width: 100%;
  font-size: 32rpx;
  border-radius: 0;
}
.tuan-btn{
  line-height: 45rpx;
  padding: 10rpx 0;
}
.tuan-btn__text{
  line-height: 35rpx;
  font-size: 26rpx;
}
.detail-bar__cell::after{
  display: none;
}
.detail-bar__orange{
  background: #ffad2c;
}
.detail-bar__red{
  background: #f24b4b;
}

button[disabled][type="default"], wx-button[disabled]:not([type]) {
  color:rgba(255, 255, 255, 1);
  background-color:#d8d8d8;
}
.select-panel{
  width: 100%;
  background: #FFF;
  position: fixed;
  bottom: 0;
  z-index: 1110;
  transform: translateY(100%);
  transition: all .3s ease-in;
}
.select-panel__active{
  transform: translateY(0%);
  transition: all .3s ease-in;
}
.select-panel-cell{
  display: flex;
  justify-content: space-around;
  padding: 40rpx 30rpx;
}
.select-text{
  font-size: 30rpx;
  flex: 1;
  height: 55rpx;
  line-height: 55rpx;
}
.select-tool{
  position: relative;
  height: 55rpx;
  flex: 1;
}
.shopcar-list__num{
  display: flex;
  height: 55rpx;
  border: 1rpx solid #bdbdbd;
  border-radius: 3rpx;
}
.shopcar-list__num-active{
  transition: all .3s ease-in;
  transform: translateX(-60rpx);
  opacity: 0;
}
.shopcar-list__num input{
  height: 55rpx;
  text-align: center;
  width: 60rpx;
  border-left: 1rpx solid #bdbdbd;
  border-right: 1rpx solid #bdbdbd;
  font-size: 30rpx;
}
.shopcar-list__num button{
  border-radius: 0;
  height: 55rpx;
  line-height: 55rpx;
  width: 60rpx;
  padding: 0;
  margin: 0;
  background: #FFF;
  color: #bdbdbd;
}
.shopcar-list__num button[disabled]{
  color:rgba(0, 0, 0, 0.3);
  background-color:#F7F7F7;
}

.shopcar-list__num button::after{
  content: '';
  border: none;
}
.select-panel__btn{
  background: #e54b44;
  color: #FFF;
  border-radius:0;
  line-height: 2.8;
}
.select-panel__btn::after{
  display: none;
}

.detail_badge{
  background: #19c2a4;
  color: #FFF;
  font-size: 26rpx;
  padding: 2rpx 4rpx;
  border-radius:4rpx; 
}

.comment{
  padding: 20rpx 15rpx 20rpx 20rpx;
  border-bottom: 1rpx solid #EFEFEF;
}
.comment-title{
  display: flex;
  justify-content: space-between;
}
.comment-name{
  font-weight: 600;
}
.comment-time{
  color: #999;
  font-size: 28rpx;
}
.comment-con{
  color: #666;
  font-size: 28rpx;
}
.eva-star{
  width: 100%;
  display: flex;
  margin-bottom: 10rpx;
}
.star-cell{
  width:25rpx;
  height: 23rpx;
  text-align: center;
  position: relative;
  margin-right: 4rpx;
}
.star-cell_icon{
  width: 25rpx;
  height: 23rpx;
  position: absolute;
  left: 50%;
  margin-left: -12rpx;
  background-image:url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat; 
}
.star-cell_icon.active{
  background-image:url(data:image/png;base64,)
}

.wxParse-inline{
  display: inline;
}