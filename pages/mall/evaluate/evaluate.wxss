@import "/utils/mall.wxss";
.container{
  padding: 0;
}
.order-list-media{
  padding:0 20rpx;
  display: flex;
  background: #FFF;
  margin-top: 20rpx;
}
.order-list-media__lt{
  width: 160rpx;
  height: 160rpx;
  padding: 20rpx 0;
}
.order-list-media__rt{
  width: 100%;
  padding: 20rpx 0;
  padding-left: 20rpx;
}

.order-list-media__lt image{
  width: 160rpx;
  height: 160rpx;
}
.order-list-media__rt__hd{
  font-size: 28rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden; 
  height: 75rpx;
}
.order-list-media__rt__bd{
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  
}
.order-list-price{
  
}
.order-list-price__now{
  font-size: 26rpx;
}
.order-list-price__now text{
  font-size: 38rpx;
}
.order-list-price__orgin{
  font-size: 22rpx;
  color: #999;
  text-decoration: line-through;
}
.order-list-count{
  color: #999;
  font-size: 26rpx;
}
.order-list__ft_price{
  font-size: 26rpx;
}
.order-list__ft_price-main{
  font-size: 36rpx;
  font-weight: bold;
}

/*评价*/
.eva-star{
  background: #FFF;
  padding:30rpx 20%;
  display: flex;
  margin-top: 20rpx;
  border-bottom: 1rpx solid #ededed;
}
.star-cell{
  width: 20%;
  text-align: center;
  text-align: center;
  position: relative;
  height: 46rpx;
}

.star-cell_icon{
  width: 50rpx;
  height: 46rpx;
  position: absolute;
  left: 50%;
  margin-left: -25rpx;
  background-image:url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat; 
}
.star-cell_icon.active{
  background-image:url(data:image/png;base64,)
}

/*快速评价*/
.fast-eva{
  display:flex;
  flex-wrap:wrap;
  justify-content:flex-start;
  padding:20rpx;
  background: #FFF;
}
.fast-eva-cell{
  padding: 10rpx 20rpx;
  margin: 15rpx 12rpx;
  font-size: 26rpx;
  border-radius:100rpx;
  border: 1rpx solid #e0e0e0;
  color: #666;
  white-space: nowrap;
}
.fast-eva-cell.active{
  color: #e54b44;
  border: 1rpx solid #e54b44;
}
.eva-textarea{
  background: #FFF;
  padding:10rpx 30rpx;
}
.eva-textarea textarea{
  background: #f8f8f8;
  border: 1rpx solid #ededed;
  width: 100%;
  padding: 20rpx;
  border-radius: 5rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  line-height: 40rpx;
}
.anonymous-eva{
  display: flex;
  justify-content: space-between;
  line-height: 40rpx;
  padding: 20rpx 0;
}
.anonymous-eva-btn{
  display: flex;
  font-size: 28rpx;
}
.anonymous-eva-btn text{
  padding-left: 20rpx;
}
.anonymous-eva-text{
  font-size: 28rpx;
  color: #999;
}
