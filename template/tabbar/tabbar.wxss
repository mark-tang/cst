
.tabbar_box{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 96rpx;
    border-top: 1rpx solid #d0d0d0; 
    background-color: #FFF;
}

.tabbar_nav{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    height: 100%;
}

.tabbar_icon{
    width: 48rpx;
    height: 48rpx;
}