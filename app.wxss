}
.fixed-btn button {
  flex: 1;
  margin: 0 15rpx;
}
.fixed-vice-btn{
  bottom: 150rpx;
}

.group-btn{
  display: flex;
  box-sizing: border-box;
}
.group-btn button {
  flex: 1;
  margin: 0 15rpx;
}

.normal-btn{
  padding: 30rpx;
  width: 100%;
  box-sizing: border-box;
}
.normal-btn button{
  box-shadow: none;
}
/*表单分割线*/
.form-cutline{
  height:20rpx;
}
/*多选列表*/
checkbox-group{

}
checkbox-group label{
  display: block;
  padding: 30rpx;
  background: #FFF;
  font-size: 30rpx;
  border-bottom: 1px solid #f8f8f8;
  display: flex;
  position: relative;
}
checkbox-group label:last-child{
  border-bottom: none;
}
checkbox-group label checkbox{

  position: absolute;
  top: 50%;
