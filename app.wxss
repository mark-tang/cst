/* dialog */
.weui-dialog {
  position: fixed;
  z-index: 1009;
  width: 80%;
  max-width: 600rpx;
  top: 25%;
  left: 50%;
  transform: translate(-50%, -50%);
  background-color: #FFF;
  border-radius: 6rpx;
  text-align: center;
}
.weui-dialog__hd {
  padding: 40rpx 50rpx 20rpx;
  position: relative;
}
.weui-dialog__bd {
  position: relative;
}
.weui-dialog__bd:not(:first-child)::after {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #e5e5e5;
  color: #e5e5e5;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  z-index: 2;
}
.weui-dialog__ft {
  display: flex;
  position: relative;
}
.weui-dialog__ft::after {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #e5e5e5;
  color: #e5e5e5;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  z-index: 2;
}
.weui-dialog__title {
  font-size: 36rpx;
}
.weui-dialog__btn {
  flex: 1;
  background-color: #FFF;
}
.weui-dialog__btn::after {
  display: none;
}
.weui-dialog__btn_primary {
  color: #3f68f7;
}

/* model-actions */
.model-actions {
  position: fixed;
  z-index: 1100;
  top: 400rpx;
  left: 0;
  bottom: 0;
  right: 0;
  -webkit-transform: translateY(100%);
  transform: translateY(100%);
  overflow: hidden;
}
.model-actions__hd {
  padding: 20rpx 30rpx;
  border-bottom: 20rpx solid #f8f8f8;
  position: absolute;
  z-index: 1000;
  top: 0;
  left: 0;
  right: 0;
  background: #FFF;
}
.model-actions__hd icon[type="close"] {
  padding: 10rpx 30rpx;
  position: absolute;
  right: 0;
  top: 0;
}
.model-actions__hd icon[type="close"]::after {
  content: "×";
  font-size: 36rpx;
}
.model-actions__title {
  font-size: 28rpx;
  color: #999;
}
.model-actions__panel {
  padding-top: 84rpx;
  position: relative;
  height: 100%;
  overflow: auto;
  box-sizing: border-box;
  background: #f8f8f8;
}

/* tab */
.tab {
  display: flex;
  text-align: center;
  border: 1rpx solid #228ade;
  border-radius: 10rpx;
  margin: 30rpx;
  font-size: 30rpx;
  overflow: hidden;
}
.tab-item {
  padding: 20rpx;
  flex: 1;
  background-color: #ffffff;
  color: #228ade;
}
.tab-item + .tab-item {
  border-left: 1rpx solid #228ade;
}
.tab-item_active {
  background-color: #228ade;
  color: #ffffff;
}
.navbar {
  position: relative;
  height: 100%;

}
.navbar .tab {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
}
.navbar .tab__panel {
  padding-top: 140rpx;
  height: 100%;
  overflow: auto;
  box-sizing:border-box;
}

/* icon */
icon[type="arrow-right"]::after {
  content: " ";
  display: inline-block;
  height: 20rpx;
  width: 20rpx;
  border-width: 1px 1px 0 0;
  border-color: #bfbfc3;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  margin-bottom: 12rpx;
  /*margin-left: -26rpx;*/
}

/* card */
.card {
  margin: 30rpx;
  border-radius: 2rpx;
  box-shadow: 0 1px 2px rgba(0,0,0,.3);
  background: #FFF;
  color: #333;
}
.card:active {
  background: #dcdcdc;
}
.card__hd {
  padding: 30rpx 20rpx;
  display: flex;
  align-items: baseline;
}
.card__bd {
  padding: 20rpx;
}
.card__title {
  display: block;
  flex: 1;
  font-size: 32rpx;
  color: #4c4c4c;
}
.card__after {
  font-size: 24rpx;
  color: #999;
}
.card__desc {
  font-size: 26rpx;
  color: #666;
  word-wrap:break-word;
  word-break: break-all;
}
.card__cover {
  padding: 0 20rpx;
  display: block;
  width: 690rpx;
  height: 300rpx;
  box-sizing: border-box;
  background-size: cover!important;
}
.card__cover:first-child {
  padding: 20rpx 20rpx 0;
}

/* badge */
.badge {
  display: inline-block;
  padding: 0 30rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  background: transparent;
  color: transparent;
  border: 1rpx solid transparent;
  border-radius: 40rpx;
}
.badge_default {
  background: #dbdbdb;
  color: #FFF;
}

/*loading*/
.loading{
  display: flex;
  justify-content: center;
  text-align: center;
  margin: 40rpx 0;
}

.loading image{
  height: 40rpx;
  width: 40rpx;
}
.loading text{
  height: 22px;
  line-height: 22px;
  font-size: 14px;
  color: #999;
  display: block;
  padding: 0 20rpx;
}
.load-icon{
  height: 18px;
  width: 18px;
  position: relative;
  border: 2px solid rgba(0,0,0,.1);
  border-radius: 50%;
  margin-left: 20rpx;
}

.load-icon::after{
  content: '';
  position: absolute;
  width: 18px;
  height: 18px;
  border: 2px solid transparent;
  border-top: 2px solid #228ade;
  border-radius: 50%;
  top: -2px;
  left: -2px;
  animation: rotate 1s ease-in-out infinite;
}
@keyframes rotate{
  0%{
    transform: rotate(0)
  }
  100%{
    transform: rotate(360deg)
  }
}

.loadover{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center; 
  color: #999;
  font-size: 28rpx;
  padding-bottom: 50rpx;
}

