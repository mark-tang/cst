}

/*右箭头*/
.arrow-right {
  position: absolute;
  right: 30rpx;
  top:50%;
  margin-top: -15rpx;
  height: 30rpx;
  width: 16rpx; 
}
.arrow-right image{
  height: 30rpx;
  width: 16rpx; 
}
.empty-list{
  position: fixed;
  top: 40%;
  left: 50%;
  transform: translate(-50%,-50%);
  text-align: center;
}
.empty-list-icon{
  width: 200rpx;
  height: 200rpx;
}
.empty-list text{
  display: block;
  color: #999;
  font-size: 28rpx;
  margin-top: 20rpx;
}

/* weui */
.weui-cells {
  margin-top: 30rpx;
  width: 100%;
  background-color: #FFFFFF;
  line-height: 1.47058824;
  font-size: 28rpx;
  color: #666;
  overflow: hidden;
  position: relative;
}
.weui-cells:first-child {
  margin-top: 0;
}
.weui-cells__title {
  margin: 20rpx 0 10rpx;
  padding: 0 30rpx;
  font-size: 28rpx;
  color: #999999;
}

.weui-cell {
  padding: 10px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.weui-cell:not(:first-child):before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #e5e5e5;
  color: #e5e5e5;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  z-index: 2;
}
.weui-cell__bd {
  flex: 1;
}
.weui-cell.weui-cell_access:active {
  background-color: #ECECEC;
}
.weui-cell.weui-cell_access .weui-cell__ft {
  padding-right: 20rpx;
  position: relative;
  text-align: right;
  color: #bfbfc3;
}
.weui-cell.weui-cell_access .weui-cell__ft:after {
  content: " ";
  display: inline-block;
  height: 10px;
  width: 10px;
  border-width: 1px 1px 0 0;
  border-color: #bfbfc3;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -4px;
  right: 2px;
}
.weui-textarea {
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  display: block;
  border: 0;
  resize: none;
  width: 100%;
  height: 240rpx;
  color: inherit;
  font-size: 1em;
  line-height: inherit;
  outline: 0;
}
.input-textarea{
  width: 100%;
  white-space: wrap;
}
.weui-label {
  display: inline-block;
  color: #999;
  width: 5em;
}
.weui-btn-area {
  margin: 50rpx 30rpx;
}
.weui-btn-area.weui-btn-fixed {
  position: fixed;
  bottom: 20rpx;
  width: 100%;
  box-sizing: border-box;
  margin:30rpx 0; 
  padding:0 30rpx;
}
.weui-vcode-btn {
  margin: -7rpx 0;
  font-size: 26rpx;
  min-width: 200rpx;
  color: #3f68f7;
}
.weui-vcode-btn::after {
  border-color: #3f68f7;
}
.weui-vcode-btn[disabled]::after {
  border-color: #dcdcdc;
}

/* text-align */
.text-left {
  text-align: left;
}
.text-center {
  text-align: center;
}
.text-right {
