/**app.wxss**/
@import "/template/tabbar/tabbar.wxss";

page{
  font-family: '微软雅黑'
}
.container {
  padding: 20rpx 0;
  box-sizing: border-box;
} 
 page {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: #f8f8f8;
  height: 100%;
} 
del {
  text-decoration: line-through;
}
.clearfix::after {
  content: " ";
  display: table;
  clear: both;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
/**表单样式**/
.form-title-tips{
  font-size: 26rpx;
  color: #9b9b9b;
  padding: 12rpx 20rpx;
  display: block;
}
.form-body{
  font-size: 30rpx;
  display:block;
}
.form-hasfixedBtn{
  margin-bottom: 140rpx;
}
.form-section{
  border-top: 1px solid #f8f8f8;
  padding: 24rpx;
  display: flex;
  position: relative;
  background-color: #FFF;
}
.form-section:first-child{
  border-top: 1px solid #FFF;
}
.form-label{
  width: 260rpx;
  height: 1.4rem;
  line-height: 1.4rem;
  color: #666;
}
.form-unit{
  position: absolute;
  right: 20rpx;
  height: 1.4rem;
  line-height: 1.4rem;
}
.form-tips-image{
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  right: 25rpx;
  top: 50%;
  margin-top: -31rpx;
  z-index: 100;
  padding: 15rpx;
}
.form-section input{
  width: 100%;
  height: 1.4rem;
  color: #333;
  padding:0px 12px;
}
.tap-input{
  width:100%;
  text-align:right;
  height:1.4rem;
  line-height:1.4rem;
  padding-right:50rpx;
}
.select-date{
  width: 100%;
  /*padding-right: 60rpx;*/
}
.input-select-view {
  padding:0rpx 12rpx;
  width:100%;
  height:1.4rem;
  line-height:1.4rem;
  text-align: left;
  display: block;
  box-sizing: border-box;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
/*placeholder样式*/
.gray-style{
  color: #d1d1d1;
}
/*按钮样式*/
button[type="primary"]{
  background-color: #228ade;
  box-shadow: 0 5rpx 8rpx  #9fb4ff;
}
.button-hover[type="primary"]{
  background-color: #3f68f7;
}
button[disabled][type="primary"]{
  background-color: #bdbdbd;
  box-shadow: 0 5rpx 8rpx  #efefef;
}
button[type="vice"]{
  color: #FFF;
  background-color: #06b0f4;
  box-shadow: 0 5rpx 8rpx  #79d9ff;
}
button[type="vice"]::after{
  border: none;
}
button[type="white"]{
  background: #FFF;
  color: #3f68f7;
  box-shadow: 0 3rpx 6rpx  rgba(61 , 102, 247, .2);
}
button[type="white"]:hover{
  background: #FFF;
  color: #3f68f7;
}
button[type="danger"]{
  color: #FFF;
  background-color: #ff4a4a;
  box-shadow: 0 5rpx 8rpx  #9fb4ff;
}
.button-hover[type="danger"]{
  color: rgba(255, 255, 255, .6);
}
button[type="secondary"]{
  color: #FFF;
  background-color: #23c5fb;
  box-shadow: 0 5rpx 8rpx  #9fb4ff;
}
.button-hover[type="secondary"]{
  color: rgba(255, 255, 255, .6);
}
button[loading][type="primary"] {
  color:rgba(255, 255, 255, 0.6);
  background-color:#bdbdbd;
}

