}
checkbox-group label text{
  height: 60rpx;
  line-height: 60rpx;
  width: 90%;
}

/*toast*/
.toast{
  position: fixed;
  top: 40%;
  z-index: 1010;
  background: rgba(0, 0, 0, .8);
  padding: 32rpx 30rpx;
  border-radius: 10rpx;
  color: #FFF;
